{
  "name": "MyCollegeRoomie",
  "version": "1.0.0-DEV",
  "buildNumber": 1,
  "private": true,
  "scripts": {
    "clean": "yarn run cleanBuildAndroid && yarn run cleanBuildIOS",
    "cleanBuildAndroid": "rm -rf android/app/build && rm -rf android/build && cd android && ./gradlew clean && cd ..",
    "cleanBuildIOS": "rm -rf ios/build",
    "android": "npx react-native run-android --verbose",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "debugApk": "yarn run clean && cd android && ./gradlew assembleDebug && cd ..",
    "releaseApk": "yarn run clean && cd android && ./gradlew assembleRelease && cd ..",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "npx patch-package",
    "updateVersion": "node node_modules/react-native-version-env-manager --updateVersion",
    "env:dev": "node node_modules/react-native-version-env-manager --env=dev --envFileExt=ts --addEnvAsPostfix",
    "env:qa": "node node_modules/react-native-version-env-manager --env=qa --envFileExt=ts --addEnvAsPostfix",
    "env:stag": "node node_modules/react-native-version-env-manager --env=stag --envTag=staging --envFileExt=ts --addEnvAsPostfix",
    "env:prod": "node node_modules/react-native-version-env-manager --env=prod --envFileExt=ts"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "https://github.com/AsadAli1/react-native-multi-slider",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/drawer": "^5.12.4",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "apisauce": "^2.0.1",
    "formik": "^2.1.5",
    "global": "^4.4.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-cli": "^2.0.1",
    "react-native-elements": "^3.2.0",
    "react-native-gesture-handler": "1.9.0",
    "react-native-image-slider-box": "^1.0.12",
    "react-native-keychain": "^6.2.0",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "^3.0.7",
    "react-native-screens": "^2.9.0",
    "react-native-simple-toast": "^1.1.2",
    "react-native-slideshow": "^1.0.1",
    "react-native-svg": "^12.1.0",
    "react-native-url-preview": "^1.1.9",
    "react-native-vector-icons": "^8.0.0",
    "react-native-version-check": "^3.4.2",
    "react-native-webview": "^11.2.5",
    "react-navigation": "latest",
    "rn-fetch-blob": "^0.12.0",
    "toggle-switch-react-native": "^3.2.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@expo/metro-config": "^0.1.57",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^26.0.20",
    "@types/react-native": "0.63.48",
    "@types/react-native-version-check": "^3.4.1",
    "@types/react-test-renderer": "16.9.5",
    "@types/rn-fetch-blob": "^1.2.1",
    "@types/yup": "^0.29.6",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "husky": "4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "metro-react-native-babel-preset": "^0.65.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-version-env-manager": "^1.2.4",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
